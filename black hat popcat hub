        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = randomPlr.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 5)
    else
        warn("æ²¡æœ‰å¯ä¼ é€çš„ç©å®¶ï¼")
    end
