-- ESP
MovementSection:Label("ğŸ‘ï¸ é€è§†åŠŸèƒ½")
MovementSection:Button("åŠ è½½é€è§†è„šæœ¬ (open source)", function()
    pcall(function()
        -- æœåŠ¡å¼•ç”¨
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local TweenService = game:GetService("TweenService")
local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

-- å…¨å±€ESPå¼€å…³çŠ¶æ€
local isESPEnabled = true

-- åˆ›å»ºESPå®¹å™¨GUI
local ESPGui = Instance.new("ScreenGui")
ESPGui.Name = "PlayerESP"
ESPGui.Parent = PlayerGui
ESPGui.IgnoreGuiInset = true

-- å¼€å…³æŒ‰é’®ï¼ˆå³ä¸Šè§’ï¼‰
local ToggleButton = Instance.new("TextButton")
ToggleButton.Name = "ESPToggle"
ToggleButton.Size = UDim2.new(0, 100, 0, 35)
ToggleButton.Position = UDim2.new(0.95, -100, 0.02, 0)
ToggleButton.BackgroundColor3 = Color3.new(0.15, 0.15, 0.15)
ToggleButton.Text = "å…³é—­ESP"
ToggleButton.TextColor3 = Color3.new(1,1,1)
ToggleButton.Font = Enum.Font.GothamBold
ToggleButton.TextSize = 14
ToggleButton.BorderSizePixel = 1
ToggleButton.BorderColor3 = Color3.new(0.5, 0.5, 0.5)
ToggleButton.AutoButtonColor = true
ToggleButton.Parent = ESPGui

-- æŒ‰é’®ç‚¹å‡»äº‹ä»¶
ToggleButton.MouseButton1Click:Connect(function()
    isESPEnabled = not isESPEnabled
    ToggleButton.Text = isESPEnabled and "å…³é—­ESP" or "å¼€å¯ESP"
    for _, playerESP in pairs(ESPGui:GetChildren()) do
        if playerESP:IsA("Frame") then
            playerESP.Visible = isESPEnabled
        end
    end
end)

-- å­˜å‚¨æ¯ä¸ªç©å®¶çš„ESPå…ƒç´ 
local PlayerESPData = {}

-- å›¢é˜Ÿé¢œè‰²æ£€æµ‹ï¼ˆä¼˜å…ˆè¯»å–TeamColorï¼Œæ— åˆ™é»˜è®¤ç™½è‰²ï¼‰
local function getTeamColor(targetPlayer)
    if targetPlayer.Team ~= nil then
        return targetPlayer.TeamColor.Color
    end
    return Color3.new(1,1,1) -- é»˜è®¤ç™½è‰²
end

-- åˆ›å»ºå•ä¸ªç©å®¶çš„ESPå…ƒç´ ï¼ˆ50x70å°ºå¯¸+ç»†æ¡†+æç»†è¡€æ¡+å›¢é˜Ÿé¢œè‰²ï¼‰
local function createPlayerESP(targetPlayer)
    if targetPlayer == Player then return end

    -- ESPä¸»å®¹å™¨ï¼ˆè°ƒæ•´ä¸º50x70ï¼‰
    local ESPFrame = Instance.new("Frame")
    ESPFrame.Name = targetPlayer.Name .. "_ESP"
    ESPFrame.Size = UDim2.new(0, 50, 0, 70) -- æ–¹æ¡†å°ºå¯¸50x70
    ESPFrame.BackgroundTransparency = 1
    ESPFrame.Parent = ESPGui
    ESPFrame.Visible = isESPEnabled

    -- æ–¹æ¡†ï¼ˆå˜ç»†ï¼šè¾¹æ¡†1åƒç´ â†’0.5è§†è§‰æ•ˆæœï¼‰
    local MainBox = Instance.new("Frame")
    MainBox.Size = UDim2.new(1, 0, 1, 0)
    MainBox.BackgroundColor3 = Color3.new(0,0,0)
    MainBox.BackgroundTransparency = 0.9
    MainBox.BorderSizePixel = 1 -- è¾¹æ¡†ç²—ç»†1åƒç´ ï¼ˆæ›´ç»†ï¼‰
    MainBox.BorderColor3 = getTeamColor(targetPlayer) -- å›¢é˜Ÿé¢œè‰²
    MainBox.Parent = ESPFrame

    -- åå­—æ ‡ç­¾ï¼ˆå±…ä¸­+å›¢é˜Ÿé¢œè‰²ï¼‰
    local NameLabel = Instance.new("TextLabel")
    NameLabel.Size = UDim2.new(1, 0, 0, 16)
    NameLabel.Position = UDim2.new(0, 0, -0.25, 0)
    NameLabel.BackgroundTransparency = 1
    NameLabel.Text = targetPlayer.Name
    NameLabel.TextColor3 = getTeamColor(targetPlayer) -- åå­—éšå›¢é˜Ÿé¢œè‰²
    NameLabel.Font = Enum.Font.Gotham
    NameLabel.TextSize = 11
    NameLabel.TextStrokeTransparency = 0.7
    NameLabel.TextStrokeColor3 = Color3.new(0,0,0)
    NameLabel.TextXAlignment = Enum.TextXAlignment.Center
    NameLabel.Parent = ESPFrame

    -- è¡€æ¡ï¼ˆæç»†ï¼šå®½åº¦3%ï¼‰
    local HealthBarContainer = Instance.new("Frame")
    HealthBarContainer.Size = UDim2.new(0.03, 0, 1, 0) -- å®½åº¦ç¼©è‡³3%ï¼ˆæç»†ï¼‰
    HealthBarContainer.Position = UDim2.new(-0.1, 0, 0, 0)
    HealthBarContainer.BackgroundColor3 = Color3.new(0.1,0.1,0.1)
    HealthBarContainer.BorderSizePixel = 1
    HealthBarContainer.BorderColor3 = Color3.new(0.4,0.4,0.4)
    HealthBarContainer.Parent = ESPFrame

    local HealthBar = Instance.new("Frame")
    HealthBar.Size = UDim2.new(1, 0, 1, 0)
    HealthBar.BackgroundColor3 = Color3.new(0,0.9,0)
    HealthBar.BorderSizePixel = 0
    HealthBar.Parent = HealthBarContainer

    -- è·ç¦»æ ‡ç­¾ï¼ˆå±…ä¸­ï¼‰
    local DistanceLabel = Instance.new("TextLabel")
    DistanceLabel.Size = UDim2.new(1, 0, 0, 14)
    DistanceLabel.Position = UDim2.new(0, 0, 1.15, 0)
    DistanceLabel.BackgroundTransparency = 1
    DistanceLabel.Text = "è·ç¦»: 0"
    DistanceLabel.TextColor3 = Color3.new(0.9,0.9,0.9)
    DistanceLabel.Font = Enum.Font.Gotham
    DistanceLabel.TextSize = 9
    DistanceLabel.TextStrokeTransparency = 0.7
    DistanceLabel.TextStrokeColor3 = Color3.new(0,0,0)
    DistanceLabel.TextXAlignment = Enum.TextXAlignment.Center
    DistanceLabel.Parent = ESPFrame

    -- å­˜å‚¨æ•°æ®
    PlayerESPData[targetPlayer] = {
        Frame = ESPFrame,
        MainBox = MainBox, -- å­˜å‚¨æ–¹æ¡†ç”¨äºæ›´æ–°å›¢é˜Ÿé¢œè‰²
        NameLabel = NameLabel, -- å­˜å‚¨åå­—æ ‡ç­¾ç”¨äºæ›´æ–°é¢œè‰²
        HealthBar = HealthBar,
        DistanceLabel = DistanceLabel,
        Humanoid = nil,
        Torso = nil
    }

    -- ç›‘å¬è§’è‰²åŠ è½½
    local function onCharacterAdded(character)
        local Humanoid = character:WaitForChild("Humanoid")
        local Torso = character:WaitForChild("HumanoidRootPart")
        PlayerESPData[targetPlayer].Humanoid = Humanoid
        PlayerESPData[targetPlayer].Torso = Torso

        -- è¡€æ¡æ›´æ–°
        Humanoid.HealthChanged:Connect(function()
            local healthRatio = Humanoid.Health / Humanoid.MaxHealth
            TweenService:Create(
                HealthBar,
                TweenInfo.new(0.2),
                {Size = UDim2.new(1, 0, healthRatio, 0)}
            ):Play()
            -- è¡€æ¡é¢œè‰²æ¸å˜
            local barColor = healthRatio > 0.5 and Color3.new(0,0.9,0) or (healthRatio > 0.2 and Color3.new(1,0.7,0) or Color3.new(0.9,0,0))
            HealthBar.BackgroundColor3 = barColor
        end)
    end

    -- ç›‘å¬å›¢é˜Ÿå˜æ›´ï¼ˆå®æ—¶æ›´æ–°é¢œè‰²ï¼‰
    targetPlayer:GetPropertyChangedSignal("Team"):Connect(function()
        local teamColor = getTeamColor(targetPlayer)
        PlayerESPData[targetPlayer].MainBox.BorderColor3 = teamColor
        PlayerESPData[targetPlayer].NameLabel.TextColor3 = teamColor
    end)

    if targetPlayer.Character then
        onCharacterAdded(targetPlayer.Character)
    end
    targetPlayer.CharacterAdded:Connect(onCharacterAdded)
end

-- ç§»é™¤ç©å®¶çš„ESPå…ƒç´ 
local function removePlayerESP(targetPlayer)
    if PlayerESPData[targetPlayer] then
        PlayerESPData[targetPlayer].Frame:Destroy()
        PlayerESPData[targetPlayer] = nil
    end
end

-- åˆå§‹åŒ–ç°æœ‰ç©å®¶
for _, targetPlayer in pairs(Players:GetPlayers()) do
    createPlayerESP(targetPlayer)
end

-- ç›‘å¬æ–°ç©å®¶åŠ å…¥
Players.PlayerAdded:Connect(createPlayerESP)

-- ç›‘å¬ç©å®¶ç¦»å¼€
Players.PlayerRemoving:Connect(removePlayerESP)

-- å®æ—¶æ›´æ–°ESPï¼ˆä¿æŒä¸‹ç§»60åƒç´ ï¼‰
RunService.RenderStepped:Connect(function()
    if not isESPEnabled then return end

    local LocalCharacter = Player.Character
    if not LocalCharacter or not LocalCharacter:FindFirstChild("HumanoidRootPart") then return end
    local LocalPosition = LocalCharacter.HumanoidRootPart.Position

    for targetPlayer, data in pairs(PlayerESPData) do
        local TargetTorso = data.Torso
        if not TargetTorso or not targetPlayer.Character then
            data.Frame.Visible = false
            continue
        end

        -- èº¯å¹²ä½ç½®è½¬å±å¹•åæ ‡ + ä¸‹ç§»60åƒç´ 
        local TargetPosition = TargetTorso.Position
        local ScreenPosition, onScreen = workspace.CurrentCamera:WorldToScreenPoint(TargetPosition)
        data.Frame.Visible = onScreen

        if onScreen then
            -- å±…ä¸­+ä¸‹ç§»60åƒç´ 
            data.Frame.Position = UDim2.new(
                0, ScreenPosition.X - (data.Frame.AbsoluteSize.X / 2),
                0, ScreenPosition.Y - (data.Frame.AbsoluteSize.Y / 2) + 60
            )

            -- æ›´æ–°è·ç¦»
            local Distance = (LocalPosition - TargetPosition).Magnitude
            data.DistanceLabel.Text = string.format("è·ç¦»: %.0f", Distance)

            -- åŒæ­¥è¡€æ¡çŠ¶æ€
            if data.Humanoid then
                local healthRatio = data.Humanoid.Health / data.Humanoid.MaxHealth
                data.HealthBar.Size = UDim2.new(1, 0, healthRatio, 0)
                local barColor = healthRatio > 0.5 and Color3.new(0,0.9,0) or (healthRatio > 0.2 and Color3.new(1,0.7,0) or Color3.new(0.9,0,0))
                data.HealthBar.BackgroundColor3 = barColor
            end
        end
    end
end)
        warn("é€è§†è„šæœ¬åŠ è½½æˆåŠŸï¼")
    end)
end)

-- ç§»åŠ¨é”åŠŸèƒ½ï¼ˆåŸæœ‰ä¿ç•™ï¼‰
MovementSection:Label("ğŸ”’ ç§»åŠ¨é”åŠŸèƒ½")
MovementSection:Button("å¯ç”¨ç§»åŠ¨é”", function()
    pcall(function()
        loadstring(game:HttpGet("https://pastebin.com/raw/LBr2RcBZ"))()
        warn("ç§»åŠ¨é”å·²å¯ç”¨ï¼")
    end)
end)

-- æ–°å¢ï¼šç¬é—´è½¬èº«
