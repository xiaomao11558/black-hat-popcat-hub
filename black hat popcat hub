-- 4. è¾…åŠ©ä¸æ‚é¡¹ä¸“åŒºï¼ˆæ–°å¢é™¤é›¾å’Œå»é™¤é˜´å½±åŠŸèƒ½ï¼‰
local UtilityTab = window:Tab("è¾…åŠ©æ‚é¡¹", "8001456789")
local UtilitySection = UtilityTab:section("å®ç”¨è¾…åŠ©åŠŸèƒ½", true)

UtilitySection:Label("ğŸ”§ åŸºç¡€è¾…åŠ©")
UtilitySection:Toggle("æ˜¾ç¤ºFPS", "å®æ—¶æ˜¾ç¤ºå¸§ç‡", false, function(state)
    local fpsGui = game.CoreGui:FindFirstChild("FPSGui")
    if state then
        if not fpsGui then
            fpsGui = Instance.new("ScreenGui")
            fpsGui.Name = "FPSGui"
            fpsGui.Parent = game.CoreGui
            local fpsLabel = Instance.new("TextLabel")
            fpsLabel.Position = UDim2.new(0.01, 0, 0.01, 0)
            fpsLabel.Size = UDim2.new(0.1, 0, 0.03, 0)
            fpsLabel.TextColor3 = Color3.new(1, 1, 1)
            fpsLabel.BackgroundTransparency = 0.5
            fpsLabel.BackgroundColor3 = Color3.new(0, 0, 0)
            fpsLabel.Parent = fpsGui
            local lastTime = tick()
            local frameCount = 0
            game:GetService("RunService").RenderStepped:Connect(function()
                if not fpsGui then return end
                frameCount = frameCount + 1
                local currentTime = tick()
                if currentTime - lastTime >= 1 then
                    fpsLabel.Text = "FPS: " .. math.floor(frameCount)
                    frameCount = 0
                    lastTime = currentTime
                end
            end)
        end
    else
        if fpsGui then
            fpsGui:Destroy()
        end
    end
end)

UtilitySection:Label("ğŸ¨ è§†è§‰è¾…åŠ©")
UtilitySection:Dropdown("è§’è‰²é¢œè‰²", "æ›´æ”¹è§’è‰²é¢œè‰²", { "çº¢è‰²", "è“è‰²", "ç»¿è‰²", "é»„è‰²", "ç´«è‰²", "é»‘è‰²", "ç™½è‰²" }, function(s)
    local colorMap = {
        ["çº¢è‰²"] = Color3.new(1, 0, 0),
        ["è“è‰²"] = Color3.new(0, 0, 1),
        ["ç»¿è‰²"] = Color3.new(0, 1, 0),
        ["é»„è‰²"] = Color3.new(1, 1, 0),
        ["ç´«è‰²"] = Color3.new(0.5, 0, 0.5),
        ["é»‘è‰²"] = Color3.new(0, 0, 0),
        ["ç™½è‰²"] = Color3.new(1, 1, 1)
    }
    if game.Players.LocalPlayer.Character then
        for _, part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Color = colorMap[s] or Color3.new(1, 1, 1)
            end
        end
    end
end)

