-- 2. ç”Ÿå­˜ä¸é˜²å¾¡ä¸“åŒºï¼ˆåŸæœ‰ä»£ç ä¸å˜ï¼‰
local SurvivalTab = window:Tab("ç”Ÿå­˜é˜²å¾¡", "78892482588180")
local SurvivalSection = SurvivalTab:section("ç”Ÿå­˜æ ¸å¿ƒåŠŸèƒ½", true)

SurvivalSection:Label("â¤ï¸ ç”Ÿå‘½å€¼ä¸é˜²å¾¡æ§åˆ¶")
SurvivalSection:Slider("ç”Ÿå‘½å€¼è°ƒæ•´", "0-100000", 100, 0, 100000, false, function(v)
    if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
        game.Players.LocalPlayer.Character.Humanoid.MaxHealth = v
        game.Players.LocalPlayer.Character.Humanoid.Health = v
    end
end)

SurvivalSection:Toggle("æ— æ•Œæ¨¡å¼", "ä¸å—ä»»ä½•ä¼¤å®³", false, function(state)
    if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
        game.Players.LocalPlayer.Character.Humanoid.MaxHealth = state and math.huge or 100
        game.Players.LocalPlayer.Character.Humanoid.Health = state and math.huge or 100
        game.Players.LocalPlayer.Character.Humanoid:SetAttribute("Invincible", state)
    end
end)

SurvivalSection:Toggle("ç©¿å¢™æ¨¡å¼", "ç©¿è¶Šå›ºä½“éšœç¢ç‰©", false, function(state)
    noclipEnabled = state
    local function noclipLoop()
        while noclipEnabled do
            task.wait()
            if game.Players.LocalPlayer.Character then
                for _, part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end
        if game.Players.LocalPlayer.Character then
            for _, part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                end
            end
        end
    end
    task.spawn(noclipLoop)
end)

SurvivalSection:Label("ğŸ›¡ï¸ é¢å¤–é˜²å¾¡åŠŸèƒ½")
SurvivalSection:Toggle("é˜²å‡»é€€", "ä¸å—å¤–åŠ›å‡»é€€", false, function(state)
    if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = state
    end
end)

-- æ–°å¢ï¼šåä½œå¼Šå…ç–«ï¼ˆåŸºç¡€é˜²æŠ¤ï¼‰
SurvivalSection:Button("å¯ç”¨åŸºç¡€åæ£€æµ‹", function()
    pcall(function()
        -- ç¦ç”¨éƒ¨åˆ†åä½œå¼Šæ£€æµ‹ç‚¹
        game:GetService("Players").LocalPlayer.PlayerGui:SetTopbarTransparency(1)
        game:GetService("RunService").Stepped:Connect(function()
            if game.Players.LocalPlayer.Character then
                game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = math.clamp(game.Players.LocalPlayer.Character.Humanoid.WalkSpeed, 0, 5000)
            end
        end)
        warn("åŸºç¡€åæ£€æµ‹å·²å¯ç”¨ï¼")
    end)
end)

-- 5. ç¢°æ’ç®±å¢å¼ºä¸“åŒºï¼ˆæ–°å¢ï¼‰
local HitboxTab = window:Tab("èŒƒå›´", "8112567890")
local HitboxSection = HitboxTab:section("ç¢°æ’ç®±æ ¸å¿ƒåŠŸèƒ½", true)
 
-- å…¨å±€å˜é‡å­˜å‚¨ç¢°æ’ç®±æ•°æ®
local hitboxEnabled = false
local hitboxSize = 10 -- é»˜è®¤ç¢°æ’ç®±å¤§å°
local originalHitboxes = {} -- å­˜å‚¨åŸå§‹ç¢°æ’ç®±æ•°æ®
local originalCanCollide = {} -- å­˜å‚¨åŸå§‹ç¢°æ’çŠ¶æ€
local renderSteppedConnection = nil -- è‡ªåŠ¨åˆ·æ–°è¿æ¥
local characterCache = {} -- è§’è‰²ç¼“å­˜ï¼Œå‡å°‘é‡å¤æŸ¥æ‰¾
 
-- åˆ›å»ºå¯è§†åŒ–ç¢°æ’ç®±ï¼ˆçº¢è‰²æ­£æ–¹å½¢ã€é€æ˜åº¦50%ã€è´´åˆèº¯å¹²ã€æ— å¡é¡¿ï¼‰
local function createVisualHitbox(character)
-- ä¼˜å…ˆè·å–èº¯å¹²éƒ¨ä»¶ï¼ˆé€‚é…R15/R6ï¼‰
local torso = character:FindFirstChild("UpperTorso") or character:FindFirstChild("Torso")
if not torso then return end
 
-- å¤ç”¨ç°æœ‰å¯è§†åŒ–ç¢°æ’ç®±ï¼ˆé¿å…é‡å¤åˆ›å»ºé”€æ¯ï¼Œå‡å°‘å¡é¡¿ï¼‰
local visualHitbox = character:FindFirstChild("VisualHitbox")
if not visualHitbox then
visualHitbox = Instance.new("Part")
visualHitbox.Name = "VisualHitbox"
visualHitbox.Anchored = false
visualHitbox.CanCollide = false
visualHitbox.Transparency = 0.5 -- ä¿æŒå¯è§ï¼Œé€æ˜åº¦50%
visualHitbox.BrickColor = BrickColor.Red()
visualHitbox.Material = Enum.Material.Plastic -- ä½æ¶ˆè€—æè´¨
visualHitbox.Parent = character
 
-- ä¸€æ¬¡æ€§åˆ›å»ºçº¦æŸï¼ˆå¤ç”¨ä¸é‡å»ºï¼‰
local weld = Instance.new("WeldConstraint")
weld.Name = "HitboxWeld"
weld.Part0 = torso
weld.Part1 = visualHitbox
weld.Parent = visualHitbox
end
 
-- ä»…æ›´æ–°å¿…è¦å±æ€§ï¼ˆå‡å°‘æ¸²æŸ“å¼€é”€ï¼‰
visualHitbox.Size = Vector3.new(hitboxSize, hitboxSize, hitboxSize)
visualHitbox.Position = torso.Position
visualHitbox.Visible = true -- å¼ºåˆ¶ä¿æŒå¯è§ï¼Œé¿å…ä¸å¯è§é—®é¢˜
end
 
-- åº”ç”¨ç¢°æ’ç®±è®¾ç½®ï¼ˆå•ä¸ªè§’è‰²ï¼Œè½»é‡æ‰§è¡Œï¼‰
local function applyHitboxToCharacter(character)
if not hitboxEnabled or not character or characterCache[character] == "processing" then return end
characterCache[character] = "processing" -- é˜²æ­¢å¹¶å‘æ‰§è¡Œ
 
-- ä¼˜å…ˆè·å–æ ¸å¿ƒéƒ¨ä»¶ï¼ˆç¼“å­˜å¸¸ç”¨éƒ¨ä»¶ï¼Œå‡å°‘æŸ¥æ‰¾å¼€é”€ï¼‰
local torso = character:FindFirstChild("UpperTorso") or character:FindFirstChild("Torso")
local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
if not torso or not humanoidRootPart then
characterCache[character] = nil
return
end
 
-- ä¿å­˜åŸå§‹æ•°æ®ï¼ˆä»…é¦–æ¬¡ä¿å­˜ï¼Œé¿å…é‡å¤æ“ä½œï¼‰
if not originalHitboxes[character] then
originalHitboxes[character] = humanoidRootPart.Size
end
if not originalCanCollide[character] then
originalCanCollide[character] = {}
-- ä»…è®°å½•æ ¸å¿ƒèº¯å¹²éƒ¨ä»¶ï¼ˆå‡å°‘æ•°æ®é‡å’Œéå†æ¶ˆè€—ï¼‰
local keyParts = {torso.Name, "HumanoidRootPart", "Head"}
for _, partName in pairs(keyParts) do
local part = character:FindFirstChild(partName)
if part and part:IsA("BasePart") then
originalCanCollide[character][partName] = part.CanCollide
end
end
end
 
-- ä»…æ›´æ–°å˜åŒ–çš„å±æ€§ï¼ˆå‡å°‘å¼•æ“é€šä¿¡å¼€é”€ï¼‰
if humanoidRootPart.Size ~= Vector3.new(hitboxSize, hitboxSize, hitboxSize) then
humanoidRootPart.Size = Vector3.new(hitboxSize, hitboxSize, hitboxSize)
end
-- è®¾ç½®æ ¸å¿ƒéƒ¨ä»¶ä¸å¯ç¢°æ’ï¼ˆæ”¯æŒç©¿è¿‡ï¼‰
local keyParts = {torso.Name, "HumanoidRootPart", "Head"}
for _, partName in pairs(keyParts) do
local part = character:FindFirstChild(partName)
if part and part:IsA("BasePart") and part.CanCollide then
part.CanCollide = false
end
end
 
-- åˆ›å»º/æ›´æ–°å¯è§†åŒ–ç¢°æ’ç®±
createVisualHitbox(character)
characterCache[character] = nil
end
 
-- æ¢å¤åŸå§‹ç¢°æ’ç®±çŠ¶æ€ï¼ˆå•ä¸ªè§’è‰²ï¼‰
local function restoreCharacterHitbox(character)
if characterCache[character] == "processing" then return end
characterCache[character] = "processing"
 
local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
if humanoidRootPart and originalHitboxes[character] then
humanoidRootPart.Size = originalHitboxes[character]
end
-- æ¢å¤ç¢°æ’çŠ¶æ€
if originalCanCollide[character] then
for partName, canCollide in pairs(originalCanCollide[character]) do
local part = character:FindFirstChild(partName)
if part and part:IsA("BasePart") then
part.CanCollide = canCollide
end
end
originalCanCollide[character] = nil
end
-- éšè—å¯è§†åŒ–ç¢°æ’ç®±ï¼ˆä¿ç•™å¯¹è±¡ï¼Œé¿å…é‡å¤åˆ›å»ºï¼‰
local visualHitbox = character:FindFirstChild("VisualHitbox")
if visualHitbox then
visualHitbox.Visible = false
end
originalHitboxes[character] = nil
characterCache[character] = nil
end
 
-- è‡ªåŠ¨åˆ·æ–°ç¢°æ’ç®±ï¼ˆè¶…ä½æ¶ˆè€—ï¼Œé¿å…å¡é¡¿ï¼‰
local function startAutoRefresh()
if renderSteppedConnection then return end
-- ä¼˜åŒ–åˆ·æ–°é¢‘ç‡ï¼š100msä¸€æ¬¡ï¼ˆå¹³è¡¡æµç•…åº¦å’Œæ€§èƒ½ï¼Œæ¯”50msæ›´çœèµ„æºï¼‰
local refreshInterval = 0.1
local lastRefreshTime = tick()
renderSteppedConnection = game:GetService("RunService").RenderStepped:Connect(function()
if not hitboxEnabled then return end
local currentTime = tick()
if currentTime - lastRefreshTime < refreshInterval then return end
lastRefreshTime = currentTime
 
-- éå†æ‰€æœ‰å…¶ä»–ç©å®¶ï¼Œè½»é‡åŒæ­¥
for _, player in pairs(game.Players:GetPlayers()) do
if player ~= game.Players.LocalPlayer and player.Character then
task.spawn(applyHitboxToCharacter, player.Character) -- å¼‚æ­¥æ‰§è¡Œï¼Œé¿å…é˜»å¡
end
end
end)
end
 
-- åœæ­¢è‡ªåŠ¨åˆ·æ–°
local function stopAutoRefresh()
if renderSteppedConnection then
renderSteppedConnection:Disconnect()
renderSteppedConnection = nil
end
-- æ¸…ç©ºç¼“å­˜
characterCache = {}
end
 
-- ç›‘å¬æ–°ç©å®¶åŠ å…¥å’Œè§’è‰²é‡ç”Ÿï¼ˆè‡ªåŠ¨é€‚é…ï¼‰
game.Players.PlayerAdded:Connect(function(player)
if player == game.Players.LocalPlayer then return end
player.CharacterAdded:Connect(function(character)
task.wait(0.3) -- ç¼©çŸ­ç­‰å¾…ï¼Œå¿«é€Ÿå“åº”
if hitboxEnabled then
task.spawn(applyHitboxToCharacter, character)
end
end)
end)
 
-- ç¢°æ’ç®±åŠŸèƒ½å¼€å…³
HitboxSection:Toggle("å¯ç”¨ç¢°æ’ç®±å¢å¼º", "å¯¹æ‰€æœ‰å…¶ä»–ç©å®¶ç”Ÿæ•ˆï¼Œå¯ç©¿è¿‡ï¼Œæ— å¡é¡¿", false, function(state)
hitboxEnabled = state
if state then
-- å¯ç”¨æ—¶åº”ç”¨åˆ°æ‰€æœ‰ç°æœ‰ç©å®¶ï¼ˆå¼‚æ­¥æ‰§è¡Œï¼Œé¿å…å¡é¡¿ï¼‰
for _, player in pairs(game.Players:GetPlayers()) do
if player ~= game.Players.LocalPlayer and player.Character then
task.spawn(applyHitboxToCharacter, player.Character)
end
end
startAutoRefresh()
warn("ç¢°æ’ç®±å¢å¼ºå·²å¯ç”¨ï¼å¯ç©¿è¿‡å…¶ä»–ç©å®¶ï¼Œå¯è§†åŒ–ä¿æŒå¯è§")
else
-- å…³é—­æ—¶æ¢å¤æ‰€æœ‰ç©å®¶çŠ¶æ€ï¼ˆå¼‚æ­¥æ‰§è¡Œï¼‰
for _, player in pairs(game.Players:GetPlayers()) do
if player ~= game.Players.LocalPlayer and player.Character then
task.spawn(restoreCharacterHitbox, player.Character)
end
end
stopAutoRefresh()
warn("ç¢°æ’ç®±å¢å¼ºå·²å…³é—­ï¼Œæ¢å¤åŸå§‹çŠ¶æ€ï¼")
end
end)
 
-- ç¢°æ’ç®±å¤§å°è°ƒèŠ‚ï¼ˆæœ€å¤§100ï¼‰
HitboxSection:Slider("ç¢°æ’ç®±å¤§å°", "é»˜è®¤10ï¼Œæœ€å¤§100", 10, 5, 100, false, function(v)
hitboxSize = v
-- è°ƒèŠ‚å¤§å°æ—¶å¼‚æ­¥åŒæ­¥ï¼Œé¿å…å¡é¡¿
if hitboxEnabled then
for _, player in pairs(game.Players:GetPlayers()) do
if player ~= game.Players.LocalPlayer and player.Character then
task.spawn(applyHitboxToCharacter, player.Character)
end
end
end
end)

