-- 1. é€šç”¨ä¸“åŒºï¼ˆåŸæœ‰ä»£ç ä¸å˜ï¼‰
local MovementTab = window:Tab("é€šç”¨", "7733774602")
local MovementSection = MovementTab:section("æ ¸å¿ƒç§»åŠ¨æ§åˆ¶", true)

MovementSection:Label("ğŸ’¨ é€Ÿåº¦ä¸è·³è·ƒè°ƒæ•´")
MovementSection:Slider("ç§»åŠ¨é€Ÿåº¦", "æœ€å¤§5000", 16, 0, 5000, false, function(v)
    if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
    end
end)

MovementSection:Slider("è·³è·ƒé«˜åº¦", "æœ€å¤§1000", 50, 0, 1000, false, function(v)
    if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
    end
end)

MovementSection:Toggle("æ— é™è·³è·ƒ", "ç©ºä¸­å¯è¿ç»­è·³è·ƒ", false, function(state)
    infiniteJumpEnabled = state
    if state then
        game:GetService("UserInputService").JumpRequest:Connect(function()
            if infiniteJumpEnabled and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
                game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
            end
        end)
    end
end)

MovementSection:Toggle("è‡ªåŠ¨å‰è¿›", "æŒç»­å‘å‰ç§»åŠ¨", false, function(state)
    autoWalkEnabled = state
    while autoWalkEnabled do
        task.wait()
        if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
            game.Players.LocalPlayer.Character.Humanoid:MoveTo(game.Players.LocalPlayer.Character.Humanoid.RootPart.CFrame * CFrame.new(0, 0, -1).Position)
        end
    end
end)

