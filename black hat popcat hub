UtilitySection:Button("é‡ç½®æ‰€æœ‰è®¾ç½®", function()
    -- é‡ç½®ç§»åŠ¨å‚æ•°
    if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("Humanoid") then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
        game.Players.LocalPlayer.Character.Humanoid.MaxHealth = 100
        game.Players.LocalPlayer.Character.Humanoid.Health = 100
        game.Players.LocalPlayer.Character.Humanoid.BodyDepthScale.Value = 1
        game.Players.LocalPlayer.Character.Humanoid.BodyHeightScale.Value = 1
        game.Players.LocalPlayer.Character.Humanoid.BodyWidthScale.Value = 1
    end
    -- å…³é—­æ‰€æœ‰å¼€å…³
    infiniteJumpEnabled = false
    noclipEnabled = false
    autoWalkEnabled = false
    fogDisabled = false
    shadowDisabled = false
    -- æ¢å¤è§†è§‰é»˜è®¤è®¾ç½®
    game.Lighting.FogEnd = originalFogEnd
    game.Lighting.GlobalShadows = originalShadowEnabled
    for _, part in pairs(workspace:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CastShadow = true
        end
    end
    -- æ¸…é™¤FPSæ˜¾ç¤º
    local fpsGui = game.CoreGui:FindFirstChild("FPSGui")
    if fpsGui then
        fpsGui:Destroy()
    end
    -- æ¢å¤é‡åŠ›é»˜è®¤å€¼
    if gravityChanged then
        workspace.Gravity = originalGravity
        gravityChanged = false
    end
    warn("æ‰€æœ‰è®¾ç½®å·²é‡ç½®ï¼")
end)

-- èœå•åº•éƒ¨ä¿¡æ¯
local InfoSection = UtilityTab:section("èœå•ä¿¡æ¯", false)
InfoSection:Label("ğŸ“Œ é»‘å¸½èœå• v1.4")
InfoSection:Label("âœ… æŒç»­æ›´æ–°å¯ç”¨åŠŸèƒ½ | 4å¤§ä¸“åŒº")
InfoSection:Label("ğŸ˜» å…³æ³¨é»‘å¸½çŒ«çŒ«è·å–æ›´å¤šåŠŸèƒ½æƒ…æŠ¥")
